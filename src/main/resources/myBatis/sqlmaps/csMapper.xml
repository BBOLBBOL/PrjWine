<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Cs">
 
   <select id="UsercsList" resultType="com.green.cs.vo.CsVo">
      SELECT *
	   FROM  USERCS CS
	   WHERE CS.U_NO = #{u_no}
	   ORDER BY CS.UCS_IDX DESC
  </select> 

</mapper>
